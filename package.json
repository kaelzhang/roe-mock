  "bugs": {
    "url": "https://github.com/kaelzhang/roe-mock/issues"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "codecov": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.18.0",
    "get-port": "^5.0.0",
    "nyc": "^14.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "next": "^8.1.0",
    "roe": "^2.1.1"
